<!DOCTYPE file PUBLIC "-//NetBeans//LaTeX Commands Description 1.0//EN" "command_description.dtd">

<file name="algpseudocode" type="package">
  <includes>
    <include name="algorithmicx" />
  </includes>

  <options>
  </options>

  <environments>
    <environment name="algorithmic">
        <commands>
            <command name="\State"/>

            <command name="\For">
              <attribute name="bracket" value="open"/>
              <attribute name="bracket-type" value="algorithmic-for"/>
              <argument type="mandatory">
                <attribute name="#code" value="true" />
              </argument>
            </command>

            <command name="\ForAll">
              <attribute name="bracket" value="open"/>
              <attribute name="bracket-type" value="algorithmic-for"/>
              <argument type="mandatory">
                <attribute name="#code" value="true" />
              </argument>
            </command>

            <command name="\EndFor">
              <attribute name="bracket" value="close"/>
              <attribute name="bracket-type" value="algorithmic-for"/>
            </command>

            <command name="\While">
              <attribute name="bracket" value="open"/>
              <attribute name="bracket-type" value="algorithmic-while"/>
              <argument type="mandatory">
                <attribute name="#code" value="true" />
              </argument>
            </command>

            <command name="\EndWhile">
              <attribute name="bracket" value="close"/>
              <attribute name="bracket-type" value="algorithmic-while"/>
            </command>

            <command name="\Until">
              <attribute name="bracket" value="open"/>
              <attribute name="bracket-type" value="algorithmic-until"/>
              <argument type="mandatory">
                <attribute name="#code" value="true" />
              </argument>
            </command>

            <command name="\Repeat">
              <attribute name="bracket" value="close"/>
              <attribute name="bracket-type" value="algorithmic-until"/>
            </command>

            <command name="\If">
              <argument type="mandatory">
                <attribute name="#code" value="true" />
              </argument>
            </command>
            <command name="\ElsIf">
              <argument type="mandatory">
                <attribute name="#code" value="true" />
              </argument>
            </command>

            <command name="\EndIf"/>
            <command name="\Else"/>

            <command name="\Procedure">
              <attribute name="bracket" value="open"/>
              <attribute name="bracket-type" value="algorithmic-procedure"/>
              <argument type="mandatory">
                <attribute name="#code" value="true" />
              </argument>
              <argument type="mandatory">
                <attribute name="#code" value="true" />
              </argument>
            </command>

            <command name="\EndProcedure">
              <attribute name="bracket" value="close"/>
              <attribute name="bracket-type" value="algorithmic-procedure"/>
            </command>

            <command name="\Function">
              <attribute name="bracket" value="open"/>
              <attribute name="bracket-type" value="algorithmic-function"/>
              <argument type="mandatory">
                <attribute name="#code" value="true" />
              </argument>
              <argument type="mandatory">
                <attribute name="#code" value="true" />
              </argument>
            </command>

            <command name="\EndFunction">
              <attribute name="bracket" value="close"/>
              <attribute name="bracket-type" value="algorithmic-function"/>
            </command>

            <command name="\Loop">
              <attribute name="bracket" value="open"/>
              <attribute name="bracket-type" value="algorithmic-loop"/>
            </command>
            <command name="\EndLoop">
              <attribute name="bracket" value="close"/>
              <attribute name="bracket-type" value="algorithmic-loop"/>
            </command>
            <command name="\Require" />
            <command name="\Ensure" />

            <command name="\Comment">
              <argument type="mandatory">
                <attribute name="#text" value="true" />
              </argument>
            </command>

        </commands>
    </environment>
  </environments>

  <commands>
  </commands>
</file>
